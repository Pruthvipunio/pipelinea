pipeline {
  agent {
  label 'Jenkins'
  }

  stages {
    stage ('BUILD') {
      steps {
        echo "This is Built stage"
        sh 'sleep 5'
      }
    }
    
    stage ('TEST') {
      steps {
        echo "This is Built stage"
        sh 'sleep 5'
      }
    }
     
    stage ('DEPLOY') {
      steps {
        echo "This is Built stage"
        sh 'sleep 5'
      }
    }
  }

}
